{
	"name": "codekaizen/wp-github-gist-block",
	"description": "WordPress plugin for embedding GitHub Gists as blocks",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"scripts": {
		"prefix-namespaces": [
			"sh -c 'test -f ./bin/strauss.phar || curl -o bin/strauss.phar -L -C - https://github.com/BrianHenryIE/strauss/releases/latest/download/strauss.phar'",
			"@php bin/strauss.phar",
			"@composer dump-autoload"
		],
		"post-install-cmd": [
			"@prefix-namespaces",
			"npm run build"
		],
		"post-update-cmd": [
			"@prefix-namespaces"
		],
		"post-autoload-dump": [
			"@php bin/strauss.phar include-autoloader"
		]
	},
	"require": {
		"scrivo/highlight.php": "^9.18"
	},
	"autoload": {
		"psr-4": {
			"CodeKaizen\\WPGithubGistBlock\\": "includes/"
		}
	},
	"extra": {
		"strauss": {
			"target_directory": "vendor",
			"namespace_prefix": "CodeKaizen\\WPGitHubGistBlock\\",
			"classmap_prefix": "CodeKaizen_WPGitHubGistBlock_",
			"constant_prefix": "CKWPGHGB_"
		}
	}
}
